{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "moduleResolution": "Node",
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
        "@gemcred/shared-ui": ["packages/shared-ui/src"]
      },
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "strict": true
